{
  "name": "midi-player-js",
  "version": "2.0.1",
  "description": "Midi parser & player engine for browser or Node. Works well with single or multitrack MIDI files.",
  "main": "module/midiplayer.js",
  "browser": "browser/midiplayer.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "mocha",
    "build": "gulp",
    "watch": "watch 'gulp' src",
    "docs": "jsdoc src README.md -d ./docs -t ./node_modules/minami"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "jsdoc": "^3.4.3",
    "minami": "^1.1.1",
    "mocha": "~2.4.5",
    "uglify-js": "^2.7.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch": "^1.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grimmdude/MidiPlayerJS.git"
  },
  "bugs": {
    "url": "https://github.com/grimmdude/MidiPlayerJS/issues"
  },
  "keywords": [
    "midi",
    "parser",
    "player"
  ],
  "author": "Garrett Grimm",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": "inline"
  }
